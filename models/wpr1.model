        <cylinder length="0.16" radius="0.3"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="20"/>
      <inertia ixx="4.00538" ixy="0.0" ixz="0.0" iyy="4.00538" iyz="0.0" izz="0.51076"/>
